{
  "name": "cognitive-complexity",
  "displayName": "Cognitive Complexity",
  "description": "Display cognitive complexity of active file in status bar",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": ["Other"],
  "activationEvents": ["onLanguage:javascript", "onLanguage:typescript"],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "cognitiveComplexity.refresh",
        "title": "Refresh Cognitive Complexity"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "typescript": "^4.9.4"
  }
}